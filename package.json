{
  "name": "react-native-redash",
  "version": "0.0.0-development",
  "description": "Utility library for React Native Reanimated",
  "main": "lib/module/index.js",
  "scripts": {
    "lint": "eslint --ext .ts,.tsx src",
    "tsc": "tsc --noEmit",
    "test": "yarn lint && yarn tsc",
    "example": "yarn --cwd Example",
    "prepare": "bob build",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wcandillon/redash.git"
  },
  "author": "William Candillon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wcandillon/redash/issues"
  },
  "homepage": "https://github.com/wcandillon/redash#readme",
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-gesture-handler": "*",
    "react-native-reanimated": "*"
  },
  "devDependencies": {
    "@react-native-community/bob": "0.7.0",
    "@types/react": "16.9.4",
    "@types/react-native": "0.60.17",
    "eslint": "6.5.0",
    "eslint-config-react-native-wcandillon": "2.1.0",
    "react": "16.9.0",
    "react-native": "0.61.1",
    "react-native-gesture-handler": "1.4.1",
    "react-native-reanimated": "1.3.0",
    "semantic-release": "15.13.24",
    "semantic-release-cli": "5.2.1",
    "typescript": "3.6.3"
  },
  "react-native": "lib/module/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "files": [
    "src",
    "lib"
  ],
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "module",
      "typescript"
    ]
  },
  "dependencies": {
    "abs-svg-path": "^0.1.1",
    "normalize-svg-path": "^1.0.1",
    "parse-svg-path": "^0.1.2",
    "use-memo-one": "^1.1.1"
  }
}
